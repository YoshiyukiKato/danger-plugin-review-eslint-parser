{
  "name": "danger-plugin-review-eslint-parser",
  "description": "A danger plugin to parser eslint result for review aggregator plugin",
  "author": {
    "name": "Yoshiyuki Kato",
    "email": "yo.shuuuuki@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YoshiyukiKato/danger-plugin-review-eslint-parser"
  },
  "bugs": {
    "url": "https://github.com/YoshiyukiKato/danger-plugin-review-eslint-parser/issues"
  },
  "homepage": "https://github.com/YoshiyukiKato/danger-plugin-review-eslint-parser#readme",
  "keywords": [
    "danger",
    "danger-plugin",
    "review"
  ],
  "version": "0.1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "$(npm bin)/tsc",
    "test": "$(npm bin)/jest",
    "predocs": "rm -rf docs/",
    "docs": "esdoc -c .esdoc.json",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "date-fns": "^1.28.5",
    "fast-glob": "^2.2.6",
    "lodash.flatten": "^4.4.0",
    "lodash.includes": "^4.3.0",
    "node-fetch": "^1.7.1",
    "semver": "^5.4.1",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.6",
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^19.2.4",
    "@types/node": "^7.0.29",
    "danger": "^7.0.15",
    "danger-plugin-review-aggregator": "^0.1.0",
    "esdoc": "^1.1.0",
    "eslint": "^5.15.3",
    "jest": "^24.5.0",
    "prettier": "^1.3.1",
    "ts-jest": "^24.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(.test)\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ]
  }
}
